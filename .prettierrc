{
  "tabWidth": 2,
  "printWidth": 80,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "importOrder": [
    "^[~]",
    "^[./]",
    "^assets/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
